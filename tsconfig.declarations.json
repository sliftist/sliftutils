{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "declaration": true,
        "emitDeclarationOnly": true,
        "outDir": "./",
        "rootDir": "./"
    },
    "include": [
        "render-utils/**/*.ts",
        "render-utils/**/*.tsx",
        "misc/**/*.ts",
        "misc/**/*.tsx",
        "storage/**/*.ts",
        "storage/**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "*_venv"
    ]
}

